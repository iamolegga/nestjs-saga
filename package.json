{
  "name": "nestjs-saga",
  "version": "0.2.0",
  "description": "Implementation of saga pattern for NestJS",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose -i --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "tsc --noemit && eslint \"{src,__tests__}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build && cp -r ./dist/* .",
    "postpublish": "git clean -fd"
  },
  "files": [
    "*.{js,d.ts}",
    "!jest.config.js",
    "!.eslintrc.js"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamolegga/nestjs-saga.git"
  },
  "keywords": [
    "nestjs",
    "nest",
    "saga"
  ],
  "author": "iamolegga <iamolegga@gmail.com> (http://github.com/iamolegga)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamolegga/nestjs-saga/issues"
  },
  "homepage": "https://github.com/iamolegga/nestjs-saga#readme",
  "devDependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/platform-express": "^10.1.3",
    "@testdeck/jest": "^0.3.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/cqrs": "^8.0.0 || ^9.0.0 || ^10.0.0"
  }
}
